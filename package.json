{
  "name": "@reactivjs/streams",
  "version": "1.0.0",
  "description": "Minimalistic and lightweight reactive objects",
  "main": "/src/stream.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node minify.js && gzip -c dist/streams.es.min.js > dist/streams.es.min.gz && cp ./src/main.d.ts ./dist/streams.es.d.ts && cp ./src/main.d.ts ./dist/streams.es.min.d.ts",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpalma95/streams.git"
  },
  "exports": {
    ".": {
      "import": "./dist/streams.es.min.js",
      "require": "./dist/strams.umd.js"
    }
  },
  "unpkg": "./dist/streams.es.min.js",
  "module": "./dist/streams.es.js",
  "keywords": [
    "reactivity"
  ],
  "author": "√Ålvaro Palma",
  "license": "MIT",
  "devDependencies": {
    "terser": "^5.24.0",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
